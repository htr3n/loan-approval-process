        </wsdl:operation>
    </wsdl:binding>

    <wsdl:service name="CallbackLoanContract">
        <wsdl:port name="CallbackLoanContractPort" binding="tns:CallbackLoanContractSOAP">
            <soap:address location="http://localhost:9999/services/CallbackLoanContract"/>
        </wsdl:port>
    </wsdl:service>

