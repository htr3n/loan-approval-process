	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:tns="urn:westbank:ws:client:CallbackLoanContract"
	targetNamespace="urn:westbank:ws:client:CallbackLoanContract">

	<wsdl:types>
