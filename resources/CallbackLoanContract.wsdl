         targetNamespace="urn:westbank:ws:client:CallbackLoanContract"
			   xmlns="urn:westbank:ws:client:CallbackLoanContract"
			   elementFormDefault="qualified">
			<xsd:element name="CallbackLoanContractRequest">
				<xsd:complexType>
					<xsd:sequence>
            <xsd:element name="borrowerCustomerId" type="xsd:long" />					
						<xsd:element name="loanContractId" type="xsd:string" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="CallbackLoanContractRequest">
		<wsdl:part element="tns:CallbackLoanContractRequest" name="request" />
	</wsdl:message>
	
	<wsdl:portType name="CallbackLoanContract">
		<wsdl:operation name="send">
